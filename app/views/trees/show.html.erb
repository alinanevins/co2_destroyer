<div class="container">
  <h1><%= @tree.name %></h1>
  <p>Diameter at breast height: <%= @tree.diameter %> cm
  <p>Mass: <%= @tree.mass %> kg
  <p>CO2 Removed: <%= @tree.co2_removed %> ppm
  <p>Owner: <%= @tree.user_id %>
  <p><%= link_to "Edit your tree", "/trees/#{@tree.id}/edit", method: 'post' %>
</div>
